<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.3 -->
<interface>
  <!-- interface-name view_data.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.5"/>
  <object class="AdwWindow" id="win_icon_view">
    <property name="content">
      <object class="AdwToolbarView">
        <property name="content">
          <object class="GtkBox">
            <property name="halign">center</property>
            <property name="orientation">vertical</property>
            <property name="spacing">5</property>
            <property name="valign">center</property>
            <child>
              <object class="GtkPicture" id="pic_icon">
                <property name="height-request">150</property>
                <property name="width-request">150</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="btn_copy">
                <property name="halign">center</property>
                <property name="hexpand">True</property>
                <property name="icon-name">edit-copy-symbolic</property>
                <property name="valign">center</property>
              </object>
            </child>
          </object>
        </property>
        <child type="top">
          <object class="AdwHeaderBar">
            <property name="title-widget">
              <object class="GtkLabel" id="lbl_title_icon">
                <property name="ellipsize">end</property>
                <property name="label">Icon Name</property>
                <property name="width-chars">30</property>
                <property name="wrap">True</property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </property>
    <property name="halign">center</property>
    <property name="resizable">False</property>
    <property name="title">Icon View</property>
    <property name="valign">center</property>
  </object>
</interface>
